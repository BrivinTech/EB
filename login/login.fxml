<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>

<StackPane prefWidth="900" prefHeight="800"  xmlns:fx="http://javafx.com/fxml" fx:controller="login.LoginController">
  <children>
    <StackPane>
      <children>
        <VBox spacing="10.0">
          <children>
            <GridPane>
              <children>
                <Label text="Username:" GridPane.columnIndex="10" GridPane.rowIndex="5" />
                <Label text="Password:" GridPane.columnIndex="10" GridPane.rowIndex="6" />
                <TextField fx:id="user" promptText="User Name"  GridPane.columnIndex="11" GridPane.rowIndex="5" />
                <TextField fx:id="password" promptText="Password"  GridPane.columnIndex="11" GridPane.rowIndex="6" />
              </children>
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="148.0" minWidth="10.0" prefWidth="109.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="228.0" minWidth="10.0" prefWidth="189.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
            </GridPane>
            <StackPane prefHeight="-1.0" prefWidth="-1.0">
              <children>
                <Button fx:id="loginButton" defaultButton="true" mnemonicParsing="false" text="Login" StackPane.alignment="CENTER_RIGHT" />
                <Button fx:id="changePasswordButton" defaultButton="true" mnemonicParsing="false" text="Change Password" StackPane.alignment="CENTER_LEFT" />
                <Label fx:id="loginStatus" text="" StackPane.alignment="CENTER" />

              </children>
            </StackPane>
          </children>
        </VBox>
      </children>
    </StackPane>
  </children>
  <padding>
    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
  </padding>
</StackPane>
